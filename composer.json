{
    "name": "lutdev/techtask",
    "description": "PHP technical task",
    "keywords": ["php", "parser"],
    "license": "MIT",
    "version": "1.0",
    "type": "project",
    "homepage": "https://github.com/lutdev/techtask",
    "readme": "README.MD",
    "authors": [
        {
            "name": "Andrew Lutskevich",
            "email": "karantin91@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "simple_html_dom.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
